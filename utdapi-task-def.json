{
    "family": "utd",
    "containerDefinitions": [
        {
            "name": "utdapi-task-definition",
            "image": "075601680455.dkr.ecr.us-east-2.amazonaws.com/utd",
            "cpu": 1,
            "memory": 100,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "command": [
                "python", "app.py"
            ],
            "essential": true
        }
    ]
}